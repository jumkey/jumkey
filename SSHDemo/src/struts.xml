<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
	<constant name="struts.i18n.encoding" value="UTF-8" />
	<package name="Base" extends="struts-default">
		<action name="listStudent" class="ListStudentAction">
			<result name="success">/WEB-INF/content/student/studentList.jsp</result>
			<result name="input">/index.jsp</result>
		</action>
		
		<action name="listTeacher" class="ListTeacherAction">
			<result name="success">/WEB-INF/content/teacher/teacherList.jsp</result>
			<result name="input">/index.jsp</result>
		</action>
		
	</package>

</struts>    
