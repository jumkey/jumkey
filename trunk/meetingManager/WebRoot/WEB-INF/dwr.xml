<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE dwr PUBLIC "-//GetAhead Limited//DTD Direct Web Remoting 2.0//EN" "http://getahead.org/dwr/dwr20.dtd">

<dwr>

  <allow>

    <create creator="spring" javascript="accountService">
    	<param name="beanName" value="SysaccountService"/>
    	<include method="updateAccountDWR" />
    </create>
    <convert converter="bean" match="com.onegrid.meetmanager.model.Sysaccount">
    	<param name="include" value="username,zip,address,id,phone" />
    </convert>

  </allow>

</dwr>
